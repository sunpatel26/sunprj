@using GridCore
@addTagHelper *, GridMvc
@model ISGrid
@{
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
    ViewBag.Title = "Home Page"; 
 }
<div class="page-breadcrumb d-none d-sm-flex align-items-center pb-2 border-bottom">
    <div class="ps-3">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb mb-0 p-0">
                <li class="breadcrumb-item">
                    <a asp-controller="Home" asp-action="Index" asp-area="Admin"><i class="bx bx-home-alt"></i></a>
                </li>
                <li class="breadcrumb-item active" aria-current="page">Companies</li>
            </ol>
        </nav>
    </div>
</div>
<div class="row pt-3">
    <div class="col-md-12">
        <grid model="@Model" />
    </div>
</div>
<script src="~/js/gridmvc.js" type="text/javascript"></script>
